{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@admin/*": ["./src/admin/*"],
            "@album/*": ["./src/album/*"],
            "@shared/*": ["./src/shared/*"]
        },
    },
}